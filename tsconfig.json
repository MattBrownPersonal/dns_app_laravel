{
    "compilerOptions": {
        "allowJs": true,
        "module": "ESNext",
        "moduleResolution": "bundler",
        "jsx": "preserve",
        "strict": true,
        "isolatedModules": true,
        "target": "ESNext",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "noEmit": true,
        "skipLibCheck": true,
        "paths": {
            "@/*": ["./resources/js/*"],
            "ziggy-js": ["./vendor/tightenco/ziggy"]
        }
    },
    "include": ["resources/js/**/*.ts", "resources/js/**/*.d.ts", "resources/js/**/*.vue", "resources/Partials/CardsComponent.vue", "resources/Partials/ExpertsComponent.vue", "resources/Partials/FooterComponent.vue", "resources/Partials/HeroComponent.vue", "resources/Partials/JumbotronComponent.vue", "resources/Partials/LogoComponent.vue", "resources/Partials/MainHeaderComponent.vue", "resources/Partials/ProblemSolversComponent.vue"]
}
